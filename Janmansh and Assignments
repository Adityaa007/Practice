#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t;
    cin >> t;
    for (int i = 0; i < t; i++) {
        int x;
        cin >> x;
        if (x + 3 > 10) {
            cout << "NO" << endl;
        }
        else
            cout << "YES" << endl;
    }

}
